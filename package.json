  "repository": {
    "type": "git",
    "url": "https://github.com/XuXiaoMao123/xxm-test-js"
  },
  "keywords": [
    "xxm",
    "typescript",
    "javascript",
    "utils",
    "toolkit",
    "helpers",
    "debounce",
    "throttle",
    "deepCopy",
    "deepMerge",
    "flattenArray",
    "flattenTree",
    "filterObject",
    "formatThousands",
    "getQueryObject",
    "getWeek",
    "handelTime",
    "isType",
    "copyTextToClipboard",
    "sortArray",
    "cleanArray",
    "convertToTree",
    "arrayToString",
